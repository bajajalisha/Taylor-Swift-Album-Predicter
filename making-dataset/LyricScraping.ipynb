    "#make data frame for lyrics\n",
    "\n",
    "lyrics_dict = {\"Artist\":[],\"Album\":[],\"TrackTitle\":[],\"Trank N\":[],\"Lyric\":[],\n",
    "               \"Year\":[]}\n",
    "\n",
    "lyrics_df = pd.DataFrame(lyrics_dict)\n",
    "\n",
    "display(lyrics_df)\n",
    "\n",
    "#functions to get album and year for each song\n",
    "\n",
    "def get_year(album):   \n",
    "    if album == lover:\n",
    "        return 2019\n",
    "    elif album == folklore:\n",
    "        return 2020\n",
    "    elif album == evermore:\n",
    "        return 2020\n",
    "\n",
    "def get_album_name(album):\n",
    "    if album == lover:\n",
    "        return \"Lover\"\n",
    "    elif album == folklore:\n",
    "        return \"Folklore\"\n",
    "    elif album == evermore:\n",
    "        return \"Evermore\"\n",
    "\n",
    "#goes through the songs FROM each album and splits rows of the song's soup into rows of the df\n",
    "\n",
    "for album in album_list:\n",
    "    \n",
    "    for songname in album:\n",
    "\n",
    "        formatted_songname = songname\n",
    "        ''.join(formatted_songname.split()).lower()\n",
    "\n",
    "        webpage = requests.get(\"https://www.azlyrics.com/lyrics/taylorswift/\"+ formatted_songname +\".html\").content\n",
    "        soup = BeautifulSoup(webpage,\"html.parser\")\n",
    "        soup_str = str(soup)\n",
    "        new_soup = soup_str.splitlines()\n",
    "\n",
    "        for lyric in new_soup:\n",
    "\n",
    "            album_name = get_album_name(album)\n",
    "            track_n = int(album.index(songname) + 1)\n",
    "            year = int(get_year(album))\n",
    "\n",
    "            new_row = {\"Artist\": \"Taylor Swift\",\"Album\": album_name,\"TrackTitle\": songname,\"Trank N\": track_n,\"Lyric\": lyric,\n",
    "               \"Year\": year}\n",
    "            lyrics_df = lyrics_df.append(new_row, ignore_index = True)\n",
    "\n",
    "display(lyrics_df)\n"
   ]
